CADENA cada, cadb, r, s;
ENTERO a, enta, entb, j, o;
ENTERO dl25, aux;
DOBLE w, z;
         v :=   x;    
        cada := cadb;
PARA(a = 98 + doblea   , doblea  <= dobleb  ,+1)
     SI( dl25 <> 989 )
     ENTONCES
          a := 25 + ( ( ( doblea  *  2.5 ) +6 )+ dobleb );
